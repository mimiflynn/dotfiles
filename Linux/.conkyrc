alignment top_right
background yes
border_width 1
cpu_avg_samples 4
default_color CCCCCC
color0 FFFFFF
default_outline_color 0077FF
default_shade_color 0077FF
double_buffer yes
draw_borders no
draw_graph_borders yes
draw_outline no
draw_shades no
gap_x 15
gap_y 15 
maximum_width 210
max_port_monitor_connections 64
max_specials 512
max_user_text 16384
minimum_size 330 10
net_avg_samples 2
no_buffers yes
out_to_console no
wm_class_name Conky
own_window_colour black
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
own_window_transparent yes
own_window_type normal
own_window yes
override_utf8_locale yes
stippled_borders 2
update_interval 1
uppercase no
use_spacer yes
use_xft yes
xftalpha 0.8
font Geogly:size=9

TEXT
${color0}Core1:$color ${cpu cpu1}%$alignr${exec sensors | grep 'Core 0' | cut -c16-22 | sed '/^$/d'}
${color0}${cpugraph cpu1 32,210 E75E4F E75E4F}
${color0}Core2:$color ${cpu cpu2}%$alignr${exec sensors | grep 'Core 1' | cut -c16-22 | sed '/^$/d'}
${color0}${cpugraph cpu2 32,210 E75E4F E75E4F}
${color0}Core3:$color ${cpu cpu3}%$alignr${exec sensors | grep 'Core 2' | cut -c16-22 | sed '/^$/d'}
${color0}${cpugraph cpu1 32,210 E75E4F E75E4F}
${color0}Core4:$color ${cpu cpu4}%$alignr${exec sensors | grep 'Core 3' | cut -c16-22 | sed '/^$/d'}
${color0}${cpugraph cpu2 32,210 E75E4F E75E4F}

${color0}RAM: $color$memperc%$alignr$mem${color0}/${color}$memmax
${color0}$membar

${color0}Down: $color${downspeed wlp4s0}$alignr${totaldown wlp4s0}
${color0}${downspeedgraph wlp4s0 32,210 2C70B7 2C70B7}

${color0}Up:   $color${upspeed wlp4s0}$alignr${totalup wlp4s0}
${color0}${upspeedgraph wlp4s0 32,210 2C70B7 2C70B7}

${color0}root$alignr$color${fs_used /}${color0}/$color${fs_size /}
${color0}${fs_bar 5,210 /}

${color0}home$alignr$color${fs_used /home}${color0}/$color${fs_size /home}
${color0}${fs_bar 5,210 /home}

${color0}swap$alignr$color${swap}${color0}/$color${swapmax}
${color0}${swapbar 5,210}

${color0}Read: $color${diskio_read}$alignr${color0}Write: $color${diskio_write}
${color0}${diskiograph /dev/sda 32,210 5FAC6D 5FAC6D}

${color0}Top:${alignr}CPU
$color${top name 1}$alignr${top cpu 1}
$color${top name 2}$alignr${top cpu 2}
$color${top name 3}$alignr${top cpu 3}
$color${top name 4}$alignr${top cpu 4}
$color${top name 5}$alignr${top cpu 5}
${color0}${alignr}MEM
$color${top_mem name 1}$alignr${top_mem mem_res 1}
$color${top_mem name 2}$alignr${top_mem mem_res 2}
$color${top_mem name 3}$alignr${top_mem mem_res 3}
$color${top_mem name 4}$alignr${top_mem mem_res 4}
$color${top_mem name 5}$alignr${top_mem mem_res 5}
